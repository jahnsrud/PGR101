<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<GridPane fx:controller="library.Controller" stylesheets="library/interface.css"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">

    <Button GridPane.rowIndex="0" text="Add Meter" onAction="#addMeter" fx:id="addButton"></Button>
    <Label GridPane.rowIndex="1" fx:id="helloLabel"></Label>
    <Label GridPane.rowIndex="2" fx:id="metersMetadataLabel"></Label>

</GridPane>